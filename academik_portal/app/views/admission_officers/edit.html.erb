<h3>Update Admission Officer</h3>

<%= @admin_officer.errors.full_messages.to_sentence %>
       <div id="ao-update">
	<%= form_for @admin_officer, url: admission_officer_path do |form| %>
	   <div class="input-name">
	<%= form.label :name %><br />
	<%= form.text_field :name %><br />
	   </div>
       <div class="input-email">
	<%= form.label :email %><br />
	<%= form.text_field :email %><br />
	   </div>
       <div class="input-password">
	<%= form.label :password %><br />
	<%= form.password_field :password %><br />
	   </div>
       <div class="ao-update-button">
	<%= form.submit "Update" %>
	   </div>
<% end %>
